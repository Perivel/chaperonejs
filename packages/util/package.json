{
  "name": "@chaperone/util",
  "version": "0.0.0",
  "type": "module",
  "types": "dist/types/core.d.ts",
  "description": "Common utility features for NodeJS projects.",
  "author": "PatrickLumenus <patrickaluy@gmail.com>",
  "homepage": "https://github.com/Perivel/chaperonejs",
  "license": "MIT",
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/esm.js",
        "require": "./dist/c.js"
      },
      "node": {
        "import": "./dist/esm.js",
        "require": "./dist/c.js"
      },
      "import": "./dist/esm.js",
      "require": "./dist/c.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib",
    "test:open": "yarn cypress open"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perivel/chaperonejs.git"
  },
  "scripts": {
    "build": "rollup --bundleConfigAsCjs -c"
  },
  "bugs": {
    "url": "https://github.com/Perivel/chaperonejs/issues"
  },
  "dependencies": {
    "awesome-phonenumber": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "change-case": "^4.1.2",
    "colors-convert": "^1.4.1",
    "i18n-iso-countries": "^7.5.0",
    "luxon": "^3.1.1",
    "ramda": "^0.28.0",
    "sanitize-html": "^2.7.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.11",
    "@types/ramda": "^0.28.20",
    "@types/uuid": "^9.0.0",
    "cypress": "^12.0.2",
    "rollup": "^3.6.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-polyfill-node": "^0.11.0"
  }
}
